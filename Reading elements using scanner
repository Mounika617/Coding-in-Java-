import java.util.*;
class Array  
{  
public static void main(String[] args)   
{   
Scanner sc=new Scanner(System.in);
int n;  
ArrayList<Integer>x=new ArrayList<Integer>();
System.out.println("enter n value");
n=sc.nextInt();
for(int i=0;i<n;i++)
x.add(sc.nextInt());
x.set(0,19);
System.out.println("updated value:"+x);
}
}
 
